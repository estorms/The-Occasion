@inject SignInManager<ApplicationUser> SignInManager
@inject UserManager<ApplicationUser> UserManager
@using Microsoft.AspNetCore.Identity

@model The_Occasion.Models.PoemViewModels.PickyPoemsViewModel
@{
    ViewData["Title"] = "Welcome to The Occasion";
}

<div class="form-group findselect" id="formselect">

    <div class="col-md-10">
        @Html.DropDownList("Forms.FormId", @Model.FormId)
    </div>
</div>

<div class="form-group findselect" id="moodselect">
    <div class="col-md-10">
        @Html.DropDownList("Moods.MoodId", @Model.MoodId)
    </div>
</div>


<div class="form-group findselect" id="topicselect">
    <div class="col-md-10">
        @Html.DropDownList("Topics.TopicId", @Model.TopicId)
    </div>
</div>
<div id="hereBeAPoem"></div>